{
  "name": "zodrq",
  "version": "1.0.4",
  "author": "kayac-chang <egg734631@gmail.com>",
  "description": "type safety request build with zod",
  "repository": "https://github.com/kayac-chang/zodrq",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
